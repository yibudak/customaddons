<?xml version="1.0" encoding="utf-8" ?>
<data>
    <record id="website_product_schema_inherit_form" model="ir.ui.view">
        <field name="name">website.product.schema.form.inherit</field>
        <field name="model">website</field>
        <field name="inherit_id" ref="website.view_website_form"/>
        <field name="arch" type="xml">
            <field name="shop_extra_field_ids" position="after">
                <group string="Product Schema">
                    <field name="brand_name"/>
                    <field name="delivery_min_time"/>
                    <field name="delivery_max_time"/>
                </group>
            </field>
        </field>
    </record>
</data>
